{
  "name": "mahoraga-leaderboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:mock": "VITE_MOCK_MODE=true vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vite build && wrangler deploy",
    "db:migrate:local": "wrangler d1 execute mahoraga-leaderboard-db --local --file=./db/schema.sql",
    "db:migrate:remote": "wrangler d1 execute mahoraga-leaderboard-db --remote --file=./db/schema.sql",
    "types": "wrangler types",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.23.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wrangler": "^4.62.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
